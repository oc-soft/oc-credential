%{
#include "service_secret.h"
#include <string.h>

/**
 * check name
 */
const char*
in_word_set(
    const char* name,
    size_t length);
%}
%%
!NAMES!
%%
/**
 * get non zero if the service need client secret id
 */
int
service_secret_need_secret(
    const char* service)
{
    int result;
    result = 0;
    if (service) {
        result = in_word_set(service, strlen(service)) != NULL;
    }
    return result;
}
